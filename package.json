  "private": true,
  "homepage": "http://stackel.github.io/cocktail-hour-web",
  "dependencies": {
    "@material-ui/core": "^3.0.0",
    "firebase": "^5.4.1",
    "lodash": "^4.17.10",
    "react": "^16.4.2",
    "react-dom": "^16.4.2",
    "react-scripts": "1.1.5",
    "react-select": "^2.0.0",
